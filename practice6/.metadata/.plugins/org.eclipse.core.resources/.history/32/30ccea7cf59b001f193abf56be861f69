package practice6;

public class Test {
	public static void main(String[] args) {
		try {
			
		Cat barsik = new Cat() ;
		Person person  = new Person(); 
		Restaurant r = new Restaurant() ; 
		System.out.println(r.servePizza(barsik));
		System.out.println(r.servePizza(person)); 
		
