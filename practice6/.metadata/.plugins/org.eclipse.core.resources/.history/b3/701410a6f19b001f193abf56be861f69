package practice6;

public abstract class Animal {
	public String name ; 
	
	public Animal() {
	}
	public Animal(String name) {
		this.name = name ; 
	}
	public String getName() {
		return this.name ; 
	}
	public abstract String breath() ; 
	public abstract String move() ;
	
	@Override
	public String toString() {
		return "name : " + name ; 
	}
	
}
