package bonus3;

public class Animal {
	
	private String name ; 
	private int age ; 
	
	public Animal(String name , int age) {
		this.name = name ; 
		this.age = age ; 
	}
	public String toString() {
		return "name " + name + " , age : " + age ; 
	}
	public String getName() {
		return this.name ; 
	}
	public int getAge() {
		return this.age ; 
	}
	public String animalDataToString() {
		return name + ";" + age + ";" ;  
	}
}
